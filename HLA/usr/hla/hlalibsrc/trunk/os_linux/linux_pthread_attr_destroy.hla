unit linuxUnit;
#include( "linux.hhf" )

procedure _pthread_attr_destroy( pa:dword );
	@cdecl;
	@external( "pthread_attr_destroy" );
	
	

// linux.pthread_attr_destroy- Destroys a pthread_attr_t object.

procedure linux.pthread_attr_destroy( var pta:linux.pthread_attr_t );
	@nodisplay; 
	@noframe;
	
begin pthread_attr_destroy;

	push( ebp );
	mov( esp, ebp );
	linux.pushregs;

	_pthread_attr_destroy( (type dword pta) );
	add( 4, esp );

	linux.popregs;
	leave();
	ret( _parms_ );	
		
end pthread_attr_destroy;


end linuxUnit;
