unit LinuxUnit;
#include( "linux.hhf" )

// shmdt - SysV share memory detach.

procedure linux.shmdt
(
		shmaddr	:dword
);
	@nodisplay;
begin shmdt;

	linux.pushregs;
	mov( linux.sys_ipc, eax );
	mov( linux.ipcop_shmdt, ebx );
	mov( shmaddr, ecx );
	int( $80 );
	linux.popregs;	
	
end shmdt;

end LinuxUnit;
