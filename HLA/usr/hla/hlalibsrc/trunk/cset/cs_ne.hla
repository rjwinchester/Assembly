unit charSets;

#include( "cset.hhf" );

/********************************/
/*                              */
/* ne-                          */
/*                              */
/* Returns true if src1 <> src2 */
/*                              */
/********************************/

procedure cs.ne( src1:cset; src2:cset ); @nodisplay; @noframe;
const
	_src1	:text := "(type dword [esp+20])";
	_src2	:text := "(type dword [esp+4])";

begin ne;

		// Stack at this point:
		//
		//	esp+20	src1	:cset
		//	esp+4	src2	:cset
		//	esp		return address
	
		mov( _src1, eax );
		cmp( eax, _src2 );
		jne NotEqual;
	
		mov( _src1[4], eax );
		cmp( eax, _src2[4] );
		jne NotEqual;
	
		mov( _src1[8], eax );
		cmp( eax, _src2[8] );
		jne NotEqual;
	
		mov( _src1[12], eax );
		cmp( eax, _src2[12] );
		
	NotEqual:
		setne( al );
		movzx( al, eax );
		ret( _parms_ );
	
end ne;


end charSets;




	

