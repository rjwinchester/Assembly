unit StdOutput;



#include( "../include/stdoutunit.hhf" )
#include( "stdio.hhf" )


/*************************************************************/
/*                                                           */
/* newln-                                                    */
/*                                                           */
/* Outputs a newline sequence to the standard output device. */
/*                                                           */
/*************************************************************/


procedure stdout.newln; @noframe;
readonly
	newLineSequence	:string := nl;
begin newln;

	stdout.puts( newLineSequence );
	ret();

end newln;



end StdOutput;




	

