unit bsdUnit;
#include( "bsd.hhf" )

procedure _pthread_mutex_unlock( mutex:dword );
	@cdecl;
	@external( "pthread_mutex_unlock" );
	
	

// bsd.pthread_mutex_unlock- Releases a mutex object.

procedure bsd.pthread_mutex_unlock( var mutex:bsd.pthread_mutex_t );
	@nodisplay; 
	@noframe;
	
begin pthread_mutex_unlock;

	push( ebp );
	mov( esp, ebp );
	bsd.pushregs;

	_pthread_mutex_unlock( (type dword mutex) );
	add( 4, esp );

	bsd.popregs;
	leave();
	ret( _parms_ );
	
end pthread_mutex_unlock;


end bsdUnit;
