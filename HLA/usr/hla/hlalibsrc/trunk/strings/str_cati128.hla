unit stringUnit;
?@nodisplay := true;
?@nostackalign := true;

#include( "strings.hhf" );
#include( "excepts.hhf" );



/**************************************************************************/
/*                                                                        */
/* str.catu128-                                                           */
/*                                                                        */
/* Appends the unsigned decimal equivalent of the "u128" parameter        */
/* to the end of the specified string.                                    */
/*                                                                        */
/**************************************************************************/

 
procedure str.cati128( dest:string; i128:lword ); 
	@noframe;
begin cati128;

	push( ebp );
	mov( esp, ebp );
	str.cati128Size( dest, i128, 0, ' ' );
	pop( ebp );
	ret( _parms_ );
		
end cati128;

end stringUnit;




	

