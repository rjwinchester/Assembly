unit mathUnit;

#include( "math.hhf" )

/*******************************************************/
/*                                                     */
/* cos32-                                              */
/*                                                     */
/* Computes the cos of the real32 object passed as a   */
/* parameter. Leaves the cos on TOS.                   */
/*                                                     */
/*******************************************************/


procedure math.cos32( r32:real32 ); @nodisplay; @noframe;
begin cos32;

	push( ebp );
	mov( esp, ebp );
	
	fld( r32 );
	math._cos();
	
	pop( ebp );
	ret( _parms_ );

end cos32;


end mathUnit;
