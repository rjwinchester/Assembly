unit mathUnit;

#include( "math.hhf" )

/*****************************************************************/
/*                                                               */
/* sincos64-                                                     */
/*                                                               */
/* Computes the sin and cos of the real32 object passed as a     */
/* parameter. Leaves the cos on TOS and the sin on NOS (FPU).    */
/*                                                               */
/*****************************************************************/


procedure math.sincos64( r64:real64 ); @nodisplay; @noframe;
begin sincos64;

	push( ebp );
	mov( esp, ebp );
	
	fld( r64 );
	math._sincos();
	
	pop( ebp );
	ret( _parms_ );

end sincos64;


end mathUnit;
