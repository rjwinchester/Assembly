unit mathUnit;

#include( "math.hhf" )

/*****************************************************************/
/*                                                               */
/* tan80-                                                        */
/*                                                               */
/* Computes the tangent of the real80 object passed as a         */
/* parameter. Leaves the tan on TOS (FPU).                       */
/*                                                               */
/*****************************************************************/


procedure math.tan80( r:real80 ); @nodisplay; @noframe;
begin tan80;

	push( ebp );
	mov( esp, ebp );
	
	fld( r );
	math._tan();
	
	pop( ebp );
	ret( _parms_ );

end tan80;


end mathUnit;
