unit FileClass;

#include( "fileclass.hhf" )
#include( "fileio.hhf" )

?@NoStackAlign := true;
?@NoDisplay := true;
?@NoFrame := true;		
		




procedure file.puti128Size
( 
	l		:lword; 
	width	:int32; 
	fill	:char 
); 
begin puti128Size;

	push( ebp );
	mov( esp, ebp );
	fileio.puti128Size( this.fileHandle, l, width, fill );
	pop( ebp );
	ret( _parms_ );
	
end puti128Size;


end FileClass;
