unit FileClass;

#include( "fileclass.hhf" )

?@NoStackAlign := true;
?@NoDisplay := true;
?@NoFrame := true;		
		




method virtualFile_t.puti128( l:lword );
begin puti128;

	push( ebp );
	mov( esp, ebp );
	this.puti128Size( l, 0, ' ' );
	pop( ebp );
	ret( _parms_ );
	
end puti128;

end FileClass;
