unit blobUnit;
?@nostackalign := true;
?@noframe := true;
?@nodisplay := true;

#includeOnce( "blobs.hhf" )
#includeOnce( "hla.hhf" )



// blob.geth32
//
//	Read a 32-bit hexadecimal integer from a blob.

procedure blob.geth32( src:blob.blob ); 
begin geth32;

	push( ebp );
	mov( esp, ebp );
	
	pushd( &blob._getc );	// Fake thunk!
	push( src );
	call hla.geth32;
	
	leave();
	ret( _parms_ );

end geth32;


	
end blobUnit;

