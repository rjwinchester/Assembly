program fgetf_test;
#include( "stdlib.hhf" )

var
	inputF		:real80;
	
begin fgetf_test;

	try
	
		stdin.getf();
		fld( 1.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1001);
		
		endif;	
		fdecstp();
		
		stdin.getf();
		fld( 2.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1002);
		
		endif;	
		fdecstp();
		
		stdin.getf();
		fld( 3.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1003);
		
		endif;	
		fdecstp();
		
		stdin.getf();
		fld( 4.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1004);
		
		endif;	
		fdecstp();
		stdin.readLn();
		stderr.put( "stdin.getf succeeded!" nl );
		os.exitProcess(0);
		
	  anyexception
	  
		stderr.put
		( 
			nl nl 
			"***************************************************" nl
			"stdin.getf failed! eax=", (type uns32 eax), nl
			"Data read did not match data expected in file" nl
			"***************************************************" nl
			nl 
		);
		stdout.put( "Press Enter to Continue: " );
		stdin.readLn();
		os.exitProcess(1);
		
	endtry;
	stderr.put
	( 
		nl nl 
		"***************************************************" nl
		"fgetf failed!" nl
		"Unknown problem with fagets test" nl
		nl 
	);
	stdout.put( "Press Enter to Continue: " );
	stdin.readLn();
	os.exitProcess(1);
			
end fgetf_test;

													