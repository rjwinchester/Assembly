all:
	hla -@@ -p:temp -obj:temp -x:tExceptions.exe tExceptions.hla
	tExceptions.exe

thread:
	hla -@@ -thread -p:temp -obj:temp -x:tExceptions.exe tExceptions.hla
	tExceptions.exe

clean:
	delete temp
	delete .inc
	delete .o
	delete .obj
	delete .link
	delete .exe
	delete .asm
	delete .pdb
	delete .ilk
	