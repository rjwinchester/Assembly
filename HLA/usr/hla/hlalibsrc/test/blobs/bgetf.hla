program bgetf_test;
#include( "stdlib.hhf" )

var
	b			:dword;
	inputF		:real80;
	
begin bgetf_test;

	try
	
		blob.a_load( "bgetf.data" );
		mov( eax, b );
		
		blob.getf( b );
		fld( 1.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1001);
		
		endif;	
		fdecstp();
		
		blob.getf( b );
		fld( 2.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1002);
		
		endif;	
		fdecstp();
		
		blob.getf( b );
		fld( 3.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1003);
		
		endif;	
		fdecstp();
		
		blob.getf( b );
		fld( 4.0 );
		fcomip( st0, st1 );
		if( @ne ) then
		
			raise(1004);
		
		endif;	
		fdecstp();
		blob.readLn( b );
		
		
		if( !blob.eof( b )) then
		
			raise(1000);
			
		endif;
		stderr.put( "blob.getf succeeded!" nl );
		os.exitProcess(0);
		
	  anyexception
	  
		stderr.put
		( 
			nl nl 
			"***************************************************" nl
			"blob.getf failed! eax=", (type uns32 eax), nl
			"Data read did not match data expected in file" nl
			"***************************************************" nl
			nl 
		);
		stdout.put( "Press Enter to Continue: " );
		stdin.readLn();
		os.exitProcess(1);
		
	endtry;
	stderr.put
	( 
		nl nl 
		"***************************************************" nl
		"blob.getf failed!" nl
		"Unknown problem with fagets test" nl
		nl 
	);
	stdout.put( "Press Enter to Continue: " );
	stdin.readLn();
	os.exitProcess(1);
			
end bgetf_test;

													