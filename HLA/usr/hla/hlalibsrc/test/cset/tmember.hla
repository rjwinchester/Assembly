program member_test;
#include( "stdlib.hhf" )

		
const
	cs_c :cset[] :=
		[
			#for( i := 0 to 126 )
				 {char(0)..char(i)},
			#endfor
			{ #0..#$7f }
		];
		
		
static
	cstest		:cset;
	ccs 		:cset[ @elements( cs_c ) ] := cs_c;
	
begin member_test;

	for( mov( 0, ecx ); ecx<@elements( cs_c ); inc(ecx) )do
	
		intmul( @size( cset ), ecx, edx );
		cs.cpy( ccs[edx], cstest );
		for( mov( 0, edx ); edx <= ecx; inc( edx )) do
		
			if( !cs.member( dl, cstest ) ) then
			
				stderr.put
				( 
					nl nl 
					"***************************************************" nl
					"cs.member #1 failed!  ecx=", ecx, " edx=", edx, nl,										
					"***************************************************" nl
					nl 
				);
				stdout.put( "Press Enter to Continue: " );
				stdin.readLn();
				os.exitProcess(1);

			endif;
			
		endfor;

		for( lea( edx, [ecx+1]); edx <= @elements( cs_c ); inc( edx )) do
		
			if( cs.member( dl, cstest ) ) then
			
				stderr.put
				( 
					nl nl 
					"***************************************************" nl
					"cs.member #2 failed!  ecx=", ecx, " edx=", edx, nl,										
					"***************************************************" nl
					nl 
				);
				stdout.put( "Press Enter to Continue: " );
				stdin.readLn();
				os.exitProcess(1);

			endif;
			
		endfor;

			
	endfor;
	stderr.put( "cs.member succeeded!" nl );
	os.exitProcess(0);
		
end member_test;

													