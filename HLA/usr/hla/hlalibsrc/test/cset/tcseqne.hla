program cseqne_test;
#include( "stdlib.hhf" )

		
const
	cseq_c :cset[] :=
		[
			#for( i := 0 to 127 )
				 {char(0)..char(i)},
			#endfor
			#for( i := 0 to 126 )
				{@char(i)..@char(127)},
			#endfor
			{ #$7f }
		];
		
	csne_c :cset[] :=
		[
			#for( i := 0 to 127 )
				 -{char(0)..char(i)},
			#endfor
			#for( i := 0 to 126 )
				-{@char(i)..@char(127)},
			#endfor
			-{ #$7f }
		];
		
		
static
	csresult	:cset;
	cseq 		:cset[ @elements( cseq_c ) ] := cseq_c;
	csne 		:cset[ @elements( csne_c ) ] := csne_c;
	
begin cseqne_test;

	
		for( mov( 0, ecx ); ecx<@elements( cseq_c ); inc(ecx) )do

			intmul( @size( cset ), ecx, edx );
		
			cs.cpy( cseq[edx], csresult );
			if( !cs.eq( csresult, cseq[edx]) ) then
			
				stderr.put
				( 
					nl nl 
					"***************************************************" nl
					"cs.eq failed!  ecx=", ecx, nl,
					(type dword csresult[12]), '_',
					(type dword csresult[8]), '_',
					(type dword csresult[4]), '_',
					(type dword csresult), nl,
					
					(type dword cseq[edx+12]), '_',
					(type dword cseq[edx+8]), '_',
					(type dword cseq[edx+4]), '_',
					(type dword cseq[edx]), nl,
										
					"***************************************************" nl
					nl 
				);
				stdout.put( "Press Enter to Continue: " );
				stdin.readLn();
				os.exitProcess(1);

			endif;
			
			if( !cs.ne( csresult, csne[edx]) ) then
			
				stderr.put
				( 
					nl nl 
					"***************************************************" nl
					"cs.ne failed!  ecx=", ecx, nl,
					(type dword csresult[12]), '_',
					(type dword csresult[8]), '_',
					(type dword csresult[4]), '_',
					(type dword csresult), nl,
					
					(type dword csne[edx+12]), '_',
					(type dword csne[edx+8]), '_',
					(type dword csne[edx+4]), '_',
					(type dword csne[edx]), nl,
								  		
					"***************************************************" nl
					nl 
				);
				stdout.put( "Press Enter to Continue: " );
				stdin.readLn();
				os.exitProcess(1);

			endif;
			
		endfor;
		stderr.put( "cs.eq & cs.ne succeeded!" nl );
		os.exitProcess(0);
		
end cseqne_test;

													