program fputcset_test;
#include( "stdlib.hhf" )

static
	f		:file;
	c		:cset := {};
	
begin fputcset_test;

	f.create();
	f.openNew( "fc_putcset.txt" );

	f.putcset( c );
	f.newln();

	or( 1, (type dword c) );
	f.putcset( c );
	f.newln();

	for( mov( 0, ecx ); ecx < 31; inc( ecx )) do

		shl( 1, (type dword c) );
		f.putcset( c );
		f.newln();

	endfor;
	mov( 0, (type dword c) );

	or( 1, (type dword c[4]) );
	f.putcset( c );
	f.newln();
	for( mov( 0, ecx ); ecx < 31; inc( ecx )) do

		shl( 1, (type dword c[4]) );
		f.putcset( c );
		f.newln();

	endfor;
	mov( 0, (type dword c[4]) );



	or( 1, (type dword c[8]) );
	f.putcset( c );
	f.newln();
	for( mov( 0, ecx ); ecx < 31; inc( ecx )) do

		shl( 1, (type dword c[8]) );
		f.putcset( c );
		f.newln();

	endfor;
	mov( 0, (type dword c[8]) );



	or( 1, (type dword c[12]) );
	f.putcset( c );
	f.newln();
	for( mov( 0, ecx ); ecx < 31; inc( ecx )) do

		shl( 1, (type dword c[12]) );
		f.putcset( c );
		f.newln();

	endfor;
	mov( 0, (type dword c[12]) );

	or( 1, (type dword c[4]));
	f.putcset( c );
	f.newln();
	for( mov( 1, ecx ); ecx < 96; inc( ecx )) do

		shl( 1, (type dword c[4]) );
		rcl( 1, (type dword c[8]) );
		rcl( 1, (type dword c[12]) );
		or( 1, (type dword c[4]) );
		f.putcset( c );
		f.newln();

	endfor;

	f.close();

end fputcset_test;

													